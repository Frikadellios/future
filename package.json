  },
  "homepage": "https://github.com/Frikadellios/future/readme",
  "engines": {
    "node": ">=18.19.1 <19 || >=20.17.0 <21 || >=21.1.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-checker -exclude \"**/node_modules/**\"",
    "browserslist": "bunx update-browserslist-db@latest",
    "build": "bun astro check --verbose && bunx --bun astro build --verbose",
    "dev": "bunx --bun astro dev --verbose",
    "start": "bun astro check --verbose && bunx --bun astro dev --verbose",
    "lint": "conc bun:lint:*",
    "lint:format": "bunx @biomejs/biome format . --write",
    "lint:fix": "bunx @biomejs/biome check . --fix",
    "lint:check": "bunx @biomejs/biome check ./* --write --unsafe"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@csstools/postcss-oklab-function": "^4.0.2",
    "@types/bun": "latest",
    "@types/mdx": "latest",
    "@types/node": "latest",
    "@types/web": "latest",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "editorconfig-checker": "^6.0.0",
    "postcss": "^8.4.47",
    "postcss-discard-comments": "^7.0.3",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "typescript": "latest",
    "bun-devtools": "latest",
    "bun-types": "latest"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
