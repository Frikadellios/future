{
  "name": "future",
  "type": "module",
  "packageManager": "bun@1.1.26",
  "private": true,
  "keywords": [
    "template",
    "biomejs",
    "bun",
    "typescript",
    "tailwindcss",
    "astrojs",
    "cloudflare",
